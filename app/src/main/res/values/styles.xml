<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="PhotoALLCheckBox">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:button">@drawable/selector_photo_all_checkbox</item>
    </style>
    <style name="PhotoCheckBox">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_marginRight">@dimen/photo_checkBox_margin</item>
        <item name="android:layout_marginBottom">@dimen/photo_checkBox_margin</item>
        <item name="android:button">@drawable/selector_photo_checkbox</item>
    </style>

</resources>
